//implemetation of linear Queue using array
#include <stdio.h>
#define MAX 5
struct Queue 
{
  int items[MAX];
  int front;
  int rear;
};

int isEmpty(Queue *q) 
{
  return q->rear < q->front;
}
void enqueue(Queue *q, int item)  
{
  if (q->rear == MAX - 1) {
  printf("Queue isfull\n");
  }
  q->items[++q->rear] = item;
}
int dequeue(Queue *q) 
{
  if (isEmpty(q)) {
  printf("Queue is empty\n");
  return -1;
  }
  return q->items[q->front++];
}
void display(Queue *q)  
{
  if (isEmpty(q)) {
  printf("Queue is empty\n");
  return;
  }
for (int i = q->front; i <= q->rear; i++) 
{
  printf("%d ", q->items[i]);
  }
  printf("\n");
}
int main()
{
  Queue q;
  q->front = 0;
  q->rear = -1;
  enqueue(&q, 10);
  enqueue(&q, 20);
  enqueue(&q, 30);
  display(&q);
  printf("Dequeued: %d\n", dequeue(&q));
  display(&q);
  return 0;
}
